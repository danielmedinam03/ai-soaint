{
  "name": "ai-soaint",
  "displayName": "gemini-soaint",
  "description": "Esta es una extensión que interactúa con un modelo Gemini para el desarrollo de código y análisis completo del proyecto",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:tuExtensionInputView"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.analyzeProject",
        "title": "Analizar proyecto completo"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tuExtensionViewContainer",
          "title": "Gemini Soaint",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "tuExtensionViewContainer": [
        {
          "type": "webview",
          "id": "tuExtensionInputView",
          "name": "Chat con Gemini"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.7.2"
  }
}